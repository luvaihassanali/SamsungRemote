<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- AppName is converted to base64 representation for id -->
		<add key="AppName" value="SamsungRemoteDemo"/>
		<!-- IP address of TV -->
		<add key="IpAddr" value="***REMOVED***"/>
		<!-- MAC address of TV (If not supplied TurnOn function cannot be used) -->
		<add key="MacAddr" value="***REMOVED***"/>
		<!-- Comm. port for TV -->
		<add key="Port" value="8002"/>
		<!-- Wifi subnet mask -->
		<add key="Subnet" value="255.255.255.0"/>
		<!-- If empty string a new token will be generated -->
		<add key="Token" value=""/>
		<!-- If true will output Debug.WriteLine statements -->
		<add key="Debug" value="True"/>
	</appSettings>
</configuration>